{
    "openapi": "3.0.0",
    "info": {
        "title": "API Specification Go Application",
        "version": "1.0.0",
        "description": "This is a sample API specification for a Go application."
    },
    "servers": [
        {
            "url": "https://localhost:8080/api",
            "description": "development server"
        }
    ],
    "components": {
        "securitySchemes": {
            "categoryAuth": {
                "type": "http",
                "scheme": "bearer",
                "bearerFormat": "JWT"
            }
        }
    },
    "paths": {
        "/auth/login": {
            "post": {
                "tags": [
                    "Auth"
                ],
                "summary": "Login User",
                "operationId": "loginUser",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "type": "object",
                                "properties": {
                                    "username": {
                                        "type": "string",
                                        "example": "User123"
                                    },
                                    "password": {
                                        "type": "string",
                                        "example": "Password123"
                                    }
                                }
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Successful login",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "message": {
                                            "type": "string",
                                            "example": "success"
                                        },
                                        "token": {
                                            "type": "string",
                                            "example": "eyJhbGciOinR5cCI6IkpXVCJ9.eyJVc2VyTm6InVzZXIxMjMiLCJzdWIiOiJhY2Nlc3NfdG9rZW4iLCJleHAiOjE3NTMxNTY0NTksImlhdCI6MTc1MzE1NjM5OX0.tQ55dxUX_8mMlbGjuZavr6Vp6I160dnrYUUgjNcPzx4"
                                        },
                                        "refresh_token": {
                                            "type": "string",
                                            "example": "iDAlpZ++pWWSDmNXfE/ex76iXxcMvG8MOdfr+wEjxMhPKYGT75UA0kHoeYRWA+zptr9n7IRwlB36hsjGKSQ03JobrYsbk2dhjqS+"
                                        }
                                    }
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "Unauthorized"
                    }
                }
            }
        },
        "/auth/refresh-token": {
            "post": {
                "tags": [
                    "Auth"
                ],
                "summary": "Login User",
                "operationId": "loginUser",
                "parameters": [
                    {
                        "name": "header",
                        "in": "header",
                        "required": true,
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "refresh_token": {
                                            "type": "string",
                                            "example": "mLQXwiWFB0PzTm+SvJrhE7+d50wvZLYGe27CF35Mrkirr4eBJdZkUCQeF3+D4ye3oYyaA6PGH803KIGt9H"
                                        }
                                    }
                                }
                            }
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successful login",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "message": {
                                            "type": "string",
                                            "example": "success"
                                        },
                                        "token": {
                                            "type": "string",
                                            "example": "eyJhbGciOinR5cCI6IkpXVCJ9.eyJVc2VyTm6InVzZXIxMjMiLCJzdWIiOiJhY2Nlc3NfdG9rZW4iLCJleHAiOjE3NTMxNTY0NTksImlhdCI6MTc1MzE1NjM5OX0.tQ55dxUX_8mMlbGjuZavr6Vp6I160dnrYUUgjNcPzx4"
                                        }
                                    }
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "Unauthorized"
                    }
                }
            }
        },
        "/api/users": {
            "get": {
                "tags": [
                    "Users"
                ],
                "summary": "Get Users",
                "operationId": "getUsers",
                "parameters": [
                    {
                        "name": "Authorization",
                        "in": "header",
                        "required": true,
                        "schema": {
                            "type": "object",
                            "properties": {
                                "x-token": {
                                    "type": "string",
                                    "example": "Bearer eyJhbGciOinR5cCI6IkpXVCJ9.eyJVc2VyTm6InVzZXIxMjMiLCJzdWIiOiJhY2Nlc3NfdG9rZW4iLCJleHAiOjE3NTMxNTY0NTksImlhdCI6MTc1MzE1NjM5OX0.tQ55dxUX_8mMlbGjuZavr6Vp6I160dnrYUUgjNcPzx4"
                                }
                            }
                        },
                        "description": "Access token in Bearer format"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successful retrieval of users",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "example": [
                                        {
                                            "id": 1,
                                            "username": "johndoe",
                                            "email": "johndoe@example.com"
                                        },
                                        {
                                            "id": 2,
                                            "username": "janedoe",
                                            "email": "janedoe@example.com"
                                        },
                                        {
                                            "id": 3,
                                            "username": "alice",
                                            "email": "alice@example.com"
                                        }
                                    ]
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "Unauthorized"
                    }
                }
            }
        }
    }
}